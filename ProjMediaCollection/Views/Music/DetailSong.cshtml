@model ProjMediaCollection.Models.MusicViewModels.DetailSongViewModel

    <form asp-action="DetailSong" asp-controller="Music" asp-route-id="@Model.Id">

        <strong>Title</strong>
        <p>@Model.Title</p>

        <strong>Artist</strong>
        <p>@Model.Artist</p>

        <strong>Release</strong>
        <p>@Model.Release</p>

        <p><strong>Genre</strong></p>
        @foreach (var item in Model.SongGenreTagsDetail)
        {
            <p>@item.Name</p>
        }

    </form>
